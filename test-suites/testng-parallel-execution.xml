<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="1"> <!-- thread-count="1" At a time how many test allowed to execute  -->
	<test thread-count="5" name="Submit Order Test">
		<classes>
			<class name="com.selenium.test.SubmitOrderAppTest" />
		</classes>
	</test>
	<test name="Error Validation Test">
		<classes>
			<class name="com.selenium.test.ErrorValidationTest" />
		</classes>
	</test>
</suite> 
